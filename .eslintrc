{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "jquery": true
    },
    "plugins": [
        "html"
    ],
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "no-var": 1,
        "no-tabs": 0,
        "no-new": 0,
        "radix": 0,
        "comma-dangle": ["error", "never"],
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "quote-props": 0,
        "quotes": 0,
        "prefer-template": 0,
        "no-plusplus": [2, {"allowForLoopAfterthoughts": true}],
        "camelcase": 0,
        "max-len": ["error", 160],
        "no-unused-expressions": 0,
        "vars-on-top": 1,
        "no-loop-func": 1,
        "space-before-function-paren": ["error", "never"],
        "object-shorthand": 0,
        "no-use-before-define": 0
    }
}
