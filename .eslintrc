{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react"
    ],
    "env": {
        "jquery": true
    },
    "rules": {
        "indent": 0,
        "linebreak-style": 0,
        "no-tabs": 0,
        "no-new": 0,
        "comma-dangle": ["error", "never"],
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "quote-props": 0
    }
}
